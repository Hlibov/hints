*{
	margin:0;
	padding:0;
}
ul, ol, dl, p, h1, h2, h3, h4, h5, h6, address, form, table, blockquote, applet, embed, object, iframe, frameset{margin:0 0 14px;}
html, body, div, span, applet, embed, object, iframe, frameset,
p, q, blockquote, sub, sup, a, abbr, acronym, font, img, small, center,
h1, h2, h3, h4, h5, h6, b, strong, big, u, ins,
i, cite, em, var, dfn, address, s, strike, del,
dl, dt, dd, ol, ul, li, pre, code, kbd, samp, tt,
fieldset, legend, form, label, table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, figcaption, figure, footer, header,
hgroup, menu, nav, section, summary, time, mark, audio,video{
	border:0;
	background:none;
}
input, textarea, select{
	font:100% arial,sans-serif;
	vertical-align:middle;
	color:#000;
	overflow:auto;
}
label{vertical-align:middle;}
button, input{
	width:auto;
	overflow:visible;
}
h1, h2, h3, h4, h5, h6, b, strong, th{font-weight:bold;}
h1{font-size:200%;}
h2{font-size:150%;}
h3{font-size:120%;}
h4{font-size:110%;}
h5{font-size:100%;}
h6{font-size:90%;}
i, cite, em, var, dfn, address{font-style:italic;}
u, ins {text-decoration:underline;}
s, strike, del{text-decoration:line-through;}
blockquote, blockquote:before, blockquote:after, q, q:before, q:after{quotes:none;}
blockquote:before, blockquote:after, q:before, q:after{content:'';}
table, caption, tbody, tfoot, thead, tr, th, td{
	border-collapse:collapse;
	border-spacing:0;
}
th, td{
	border:1px solid #ccc;
	padding:3px 7px;
	text-align:left;
	vertical-align:top;
}
th{
	border-color:#999;
	text-align:center;
	vertical-align:middle;
}
ul{margin-left:1px;}
ul li, ol li{list-style-position:inside;}
ul li{list-style-type:disc;}
body{
	-webkit-text-size-adjust:100%;
	-ms-text-size-adjust:none;
	margin:0;
	min-width:320px;
	font:12px/19px Arial, Helvetica, sans-serif;
	color:#000;
	background:#e4e3e3;
}
img{max-width:100%;}
a{
	color:#ee3123;
	text-decoration:none;
}
a:hover{text-decoration:underline;}
a:active{background-color:transparent;}
input[type=text], input[type=password], input[type=file], input[type=submit], textarea{-webkit-appearance:none;}
#header:after{
	content:'';	display:block;	clear:both;
}
#wrapper{
	overflow:hidden;
	background:#f8f8f8 url(../images/bg-wrapper.png) repeat-x;
}
.w1{
	max-width:940px;
	margin:0 auto;
}
#header{
	position:relative;
	z-index:8;
	padding:33px 0 26px 36px;
}
.logo{	float:left;	width:155px;	height:42px;	margin:0 2px 0 0;	text-indent:-9999px;	overflow:hidden;	background:url(../images/sprite.png) no-repeat;}.logo a{	display:block;	height:100%;}
#nav{
	float:right;
	width:622px;
	margin:0;
	padding:10px 0 0;
	line-height:16px;
	text-transform:uppercase;
}
#nav>li{
	list-style:outside none;
	position:relative;
	float:left;
	padding:0 0 25px;
	margin-left:34px;
}
#nav>li:after{
	position:absolute;
	content:'';
	left:-18px;
	top:-36px;
	width:2px;
	height:100px;
	background:url(../images/sprite.png) no-repeat -160px 0;
}
#nav>li:first-child{margin-left:0;}
#nav>li:first-child:after{display:none;}
#nav a{
	display:block;
	position:relative;
	color:#fff;
}
#nav>li.hover>a{text-decoration:none;}
#nav .text-hover{display:none;}
#nav>li.hover>a .text-hover{
	position: absolute;
	display:block;
	content:"";
	left: 0;
	top: 12px;
	width: 100%;
	height: 100%;
	-moz-transform: scaleY(-1);
	-o-transform: scaleY(-1);
	-webkit-transform: scaleY(-1);
	-ms-transform: scaleY(-1);
	transform: scaleY(-1);
	 z-index: 1;
}
#nav>li.hover>a:before{
	content: "";
	display: block;
	width: 102%;
	height: 80%;
	position: absolute;
	left: -1px;
	top: 12px;
	z-index: 2;
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  rgba(187,33,28,0.7) 0%, rgba(187,33,28,0.98) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(187,33,28,0.7)), color-stop(100%,rgba(187,33,28,0.98)));
	background: -webkit-linear-gradient(top,  rgba(187,33,28,0.7) 0%,rgba(187,33,28,0.98) 100%);
	background: -o-linear-gradient(top,  rgba(187,33,28,0.7) 0%,rgba(187,33,28,0.98) 100%);
	background: -ms-linear-gradient(top,  rgba(187,33,28,0.7) 0%,rgba(187,33,28,0.98) 100%);
	background: linear-gradient(to bottom,  rgba(187,33,28,0.7) 0%,rgba(187,33,28,0.98) 100%);
}
#nav .drop{
	position:absolute;
	z-index:9;
	left:-28px;
	top:-9999px;
	width:999px;
	padding:16px 15px 14px 14px;
	background:#8d8c8c;
	background:#8d8c8c url(data:image/svg+xml;base64,);
	background:-moz-linear-gradient(top,  #8d8c8c 0%, #908f8f 23%, #9c9c9c 73%, #a0a0a0 100%);
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0%,#8d8c8c), color-stop(23%,#908f8f), color-stop(73%,#9c9c9c), color-stop(100%,#a0a0a0));
	background:-webkit-linear-gradient(top,  #8d8c8c 0%,#908f8f 23%,#9c9c9c 73%,#a0a0a0 100%);
	background:-o-linear-gradient(top,  #8d8c8c 0%,#908f8f 23%,#9c9c9c 73%,#a0a0a0 100%);
	background:-ms-linear-gradient(top,  #8d8c8c 0%,#908f8f 23%,#9c9c9c 73%,#a0a0a0 100%);
	background:linear-gradient(to bottom,  #8d8c8c 0%,#908f8f 23%,#9c9c9c 73%,#a0a0a0 100%);
}
#nav li.hover>.drop{
	display:block;
	top:100%
}
#nav .drop>.holder{float:left;}
#nav .drop ul{
	float:left;
	width:190px;
	margin:0 0 0 22px;
}
#nav .drop ul:first-child{margin-left:0;}
#nav .drop li{
	list-style:outside none;
	position:relative;
	border-bottom:1px solid #a2a2a2;
	border-bottom:1px solid rgba(255,255,255,0.08);
}
#nav .drop li:first-child{
	border-top:1px solid #a2a2a2;
	border-top:1px solid rgba(255,255,255,0.08);
}
#nav .drop a{padding:8px 16px 7px;}
#nav .drop .hover>a{
	text-decoration:none;
	background:#a0a0a0;
	background:#a0a0a0 url(data:image/svg+xml;base64,);
	background:-moz-linear-gradient(top,  #a0a0a0 0%, #8f8e8e 100%);
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0%,#a0a0a0), color-stop(100%,#8f8e8e));
	background:-webkit-linear-gradient(top,  #a0a0a0 0%,#8f8e8e 100%);
	background:-o-linear-gradient(top,  #a0a0a0 0%,#8f8e8e 100%);
	background:-ms-linear-gradient(top,  #a0a0a0 0%,#8f8e8e 100%);
	background:linear-gradient(to bottom,  #a0a0a0 0%,#8f8e8e 100%);
}
#nav .drop .drop{
	position:absolute;
	z-index:10;
	left:100%;
	padding:0 0 0 14px;
	background:none;
}
#nav .drop li.hover>.drop{top:-17px;}
#nav .drop ul ul{
	width:214px;
	padding:16px 15px 14px 14px;
	margin:0;
	background:#8d8c8c;
	background:#8d8c8c url(data:image/svg+xml;base64,);
	background:-moz-linear-gradient(top,  #8d8c8c 0%, #908f8f 23%, #9c9c9c 73%, #a0a0a0 100%);
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0%,#8d8c8c), color-stop(23%,#908f8f), color-stop(73%,#9c9c9c), color-stop(100%,#a0a0a0));
	background:-webkit-linear-gradient(top,  #8d8c8c 0%,#908f8f 23%,#9c9c9c 73%,#a0a0a0 100%);
	background:-o-linear-gradient(top,  #8d8c8c 0%,#908f8f 23%,#9c9c9c 73%,#a0a0a0 100%);
	background:-ms-linear-gradient(top,  #8d8c8c 0%,#908f8f 23%,#9c9c9c 73%,#a0a0a0 100%);
	background:linear-gradient(to bottom,  #8d8c8c 0%,#908f8f 23%,#9c9c9c 73%,#a0a0a0 100%);
}
#nav .drop .drop a{
	width:198px !important;
	padding-right:0;
}

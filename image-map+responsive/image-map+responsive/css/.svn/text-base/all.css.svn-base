.text-box{
	overflow:hidden;
	padding:2px 20px 8px 24px;
	line-height:14px;
}
.text-box .alignleft{
	max-width:63%;
	margin-right:17px;
}
.text-box .alignright{margin-left:17px;}
#main .text-box h2{margin-bottom:20px;}
#main .text-box ul{
	padding:14px 0 0;
	line-height:16px;
}
#main .photo-gallery{
	margin:0 -2px 0 0;
	padding:0 0 15px 22px;
}
#main .photo-gallery li{
	float:left;
	overflow:hidden;
	padding:0;
	margin:0 2.4% 20px 0;
}
#main .photo-gallery li:after{display:none;}
#main .photo-gallery li.width01{width:47.5%;}
#main .photo-gallery li.width02{width:24.5%;}
#main .photo-gallery li.width03{width:20.8%;}
#main .photo-gallery img{vertical-align:top;}
#main .photo-gallery a{
	position:absolute;
	left:0;
	bottom:0;
	width:100%;
	box-sizing:border-box;	-moz-box-sizing:border-box;	-webkit-box-sizing:border-box;
	padding:11px 13px 8px;
	color:#fff;
	background:#3b3b3b;
	background:rgba(59,59,59,0.8);
}
.text-frame{
	position:relative;
	overflow:hidden;
	padding:0 0 10px;
	margin:0 0 27px;
	border-bottom:1px solid #fff;
}
.text-frame:after{
	position:absolute;
	content:'';
	left:0;
	bottom:0;
	width:100%;
	height:1px;
	background:#d8d8d8;
}
.text-frame>.holder{
	overflow:hidden;
	padding:0 0 13px;
}
.articles-holder{
	overflow:hidden;
	padding:0 0 67px;
	line-height:0;
}
.articles-holder:after{
	display:inline-block;
	vertical-align:top;
	content:'';
	width:100%;
}
.articles-holder .article{
	display:inline-block;
	vertical-align:top;
	width:23.3%;
	line-height:14px;
}
#main .articles-holder h2{margin-bottom:20px;}
.articles-holder .article img{
	vertical-align:top;
	margin:0 0 15px;
}
#main .articles-holder p{margin:0 0 26px;}
.btn-more{
	display:inline-block;
	vertical-align:top;
	padding:5px 18px 4px;
	font-size:12px;
	line-height:16px;
	color:#fff;
	text-transform:uppercase;
	background:#e32822;
	background:#e32822 url(data:image/svg+xml;base64,);
	background:-moz-linear-gradient(top,  #e32822 0%, #d72620 25%, #af1f1a 79%, #a41d18 100%);
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0%,#e32822), color-stop(25%,#d72620), color-stop(79%,#af1f1a), color-stop(100%,#a41d18));
	background:-webkit-linear-gradient(top,  #e32822 0%,#d72620 25%,#af1f1a 79%,#a41d18 100%);
	background:-o-linear-gradient(top,  #e32822 0%,#d72620 25%,#af1f1a 79%,#a41d18 100%);
	background:-ms-linear-gradient(top,  #e32822 0%,#d72620 25%,#af1f1a 79%,#a41d18 100%);
	background:linear-gradient(to bottom,  #e32822 0%,#d72620 25%,#af1f1a 79%,#a41d18 100%);
}
.main-bar{
	width:100%;
	margin:0 -9999px;
	padding:20px 9999px 93px;
	overflow:hidden;
	line-height:16px;
	background:#fff;
}
#main .main-bar h2{margin:0 0 11px;}
.main-bar .box-about{
	float:right;
	width:49%;
	margin:0 0 0 4%;
}
.box-about .alignleft, .box-about .alignleft{margin-top:3px;}
.main-bar .links-box{
	overflow:hidden;
	padding:0 0 0 5px;
}
.main-bar .list-holder{
	overflow:hidden;
	margin:-2px 0 0;
}
#main .list-holder ul{
	float:left;
	width:40%;
	margin:0 0 0 13%;
}
#main .list-holder ul:first-child{margin:0;}
#main .list-holder ul li{padding:0 0 10px 24px;}
#footer{
	max-width:940px;
	margin:0 auto;
	padding:9px 0 6px;
}
#footer .partners-list{
	margin:0;
	text-align:justify;
	line-height:0;
}
#footer .partners-list:after{
	display:inline-block;
	vertical-align:middle;
	content:'';
	width:100%;
}
#footer .partners-list li{
	list-style:outside none;
	display:inline-block;
	vertical-align:middle;
	padding:0 0 4px;
}
#footer .partners-list img{
	vertical-align:middle;
	width:auto;
}
